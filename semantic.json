{
  "base": "node_modules/semantic-ui",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": true,
  "rtl": false,
  "components": [
    "accordion",
    "api",
    "checkbox",
    "dimmer",
    "dropdown",
    "form",
    "modal",
    "popup",
    "progress",
    "search",
    "sidebar",
    "transition"
  ],
  "version": "2.2.13"
}
